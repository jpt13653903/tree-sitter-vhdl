{
  "devDependencies": {
    "tree-sitter-cli": "^0.22.6",
    "prebuildify": "^6.0.0"
  },
  "name": "tree-sitter-vhdl",
  "version": "1.0.0",
  "description": "VHDL grammar for tree-sitter",
  "main": "grammar.js",
  "types": "bindings/node",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tree-sitter test",
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jpt13653903/tree-sitter-vhdl.git"
  },
  "keywords": [
    "vhdl"
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ],
  "author": "John-Philip Taylor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jpt13653903/tree-sitter-vhdl/issues"
  },
  "homepage": "https://github.com/jpt13653903/tree-sitter-vhdl#readme",
  "dependencies": {
    "node-gyp-build": "^4.8.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.0"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "tree-sitter": [
    {
      "scope": "source.vhdl",
      "file-types": ["vhd", "vhdl"],
      "injection-regex": "^vhdl$",
      "highlights": "queries/highlights.scm"
    }
  ]
}
